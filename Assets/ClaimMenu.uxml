            <engine:Button name="ClaimGeneral" text="Claim General Reward" style="font-size: 18px; font-weight: bold; background-color: rgba(0, 0, 0, 0.5); color: white; margin-bottom: 10px;"></engine:Button>
            <engine:Button name="ClaimDeputy" text="Claim Deputy Reward" style="font-size: 18px; font-weight: bold; background-color: rgba(0, 0, 0, 0.5); color: white; margin-bottom: 10px;"></engine:Button>
            <engine:Button name="ClaimBoss" text="Claim Boss Reward" style="font-size: 18px; font-weight: bold; background-color: rgba(0, 0, 0, 0.5); color: white; margin-bottom: 10px;"></engine:Button>
