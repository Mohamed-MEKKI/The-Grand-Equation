        <!-- Background -->
        <engine:Image name="Background" style="background-image: url('Assets/Resources/ClaimMenuBackground.png'); background-size: cover; width: 100%; height: 100%;"></engine:Image>
        
        <!-- Menu Items -->
        <engine:VisualElement name="MenuContainer" style="flex-direction: column; align-items: center; padding: 20px;">
